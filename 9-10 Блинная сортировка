def bubble_sort(arr):
    n = len(arr)
    for i in range(n):
        # Проходим по массиву, сравниваем соседние элементы и меняем их местами, если нужно
        for j in range(0, n - i - 1):
            if arr[j] > arr[j + 1]:
                arr[j], arr[j + 1] = arr[j + 1], arr[j]
    return arr

# Пример использования
array = [64, 34, 25, 12, 22, 11, 90]
print(bubble_sort(array))
